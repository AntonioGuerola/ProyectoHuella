<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="600.0" prefWidth="900.0"
            xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.views.mainController"
            stylesheets="@../styles/mainStyle.css">

    <center>
        <AnchorPane prefHeight="600.0" prefWidth="900.0">
            <VBox spacing="15.0" alignment="CENTER" layoutX="350.0" layoutY="250.0" prefWidth="200.0">
                <Button fx:id="btnRegistrarHuella" onAction="#goRegistrarHuella" text="Registrar Huella"/>
                <Button fx:id="btnRegistrarHabito" onAction="#goRegistrarHabito" text="Registrar Hábito"/>
                <Button fx:id="btnConsultarHuellas" onAction="#goConsultarHuellas" text="Consultar Huellas"/>
                <Button fx:id="btnRecomendaciones" onAction="#goRecomendaciones" text="Recomendaciones"/>
                <Button fx:id="btnClose" onAction="#goBack" text="Cerrar Sesión"/>
            </VBox>
        </AnchorPane>
    </center>

</BorderPane>
